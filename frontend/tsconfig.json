{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
    	"target": "ES2022",
    	"verbatimModuleSyntax": true,
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
		"moduleResolution": "bundler"
	}
}
